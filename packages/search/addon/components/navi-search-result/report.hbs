{{#if @data}}
<h1>{{this.title}}</h1>
<ul class="search-result">
    {{#each @data.reports as |result|}}
    <li>{{result.title}}</li>
    {{/each}}
    {{#each @data.dashboards as |result|}}
    <li>{{result.title}}</li>
    {{/each}}
</ul>
{{/if}}
