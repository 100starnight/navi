{{#if (or (await @data.data.reports.content) (await @data.data.dashboards.content))}}
<h1>{{this.title}}</h1>
<ul class="search-result">
    {{#each (await @data.data.reports) as |result|}}
    <li>{{result.title}}</li>
    {{/each}}
    {{#each (await @data.data.dashboards) as |result|}}
    <li>{{result.title}}</li>
    {{/each}}
</ul>
{{/if}}
