{{#if searchResult.content}}
<h1>{{this.title}}</h1>
<div class="search-result">
    <ul>
        {{#each searchResult as |result|}}
        <li>{{result.title}}</li>
        {{/each}}
    </ul>
</div>
{{/if}}
