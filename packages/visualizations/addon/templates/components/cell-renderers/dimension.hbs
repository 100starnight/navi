{{#with (get data (concat column.field.dimension '|desc' )) as |description|}}
    <span title={{description}}>{{description}}</span>
{{else}}
    {{#with (get data (concat column.field.dimension '|id' )) as |id|}}
        <span title={{id}}>{{id}}</span>
    {{else}}
        --
    {{/with}}
{{/with}}
